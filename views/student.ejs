<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
    <style>
table {
    border-collapse: collapse;
    width: 100%;
    font-size: 14px;
}

td, th {
    border: 1px solid black;
    text-align: center;
    padding: 10px;
}

tr:nth-child(even){
  background-color: #f2f2f2
}

th {
    background-color: #4CAF50;
    color: white;
}
</style>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>id</th>
          <th>Frist name</th>
          <th>Last name</th>
          <th>Email</th>
        </tr>
      </thead>
    <tbody>
      <%data.forEach( row =>{ %>
      <tr>
        <td><%= row.id %></td>
        <td><%= row.first_name %></td>
        <td><%= row.last_name %></td>
        <td><%= row.email %></td>
      </tr>
      <%})%>
  </tbody>
</table>
<button type="button" onclick="alert('Hello world!')">Click Me!</button>
<form class="" action="/student/" method="post">
  <input type="text" name="first_name" value=""placeholder="input nama">
  <input type="text" name="last_name" value=""placeholder="input last name">
  <input type="text" name="email" value=""placeholder="input email">
  <input type="submit" name="" value="submit">

</form>

  </body>
</html>
